import java.net.CookieManager;
import java.util.Scanner;

public class phuongTrinhBacHai {
	public void kiemTraNamNhuan(int a) {
		if (a%4==0 || a%400==0) {
			System.out.println("Co 29 ngay!");
		}else {
			System.out.println("Co 28 ngay!");
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Nhap thang");
		int thang = sc.nextInt();
		System.out.println("Nhap nam");
		int nam = sc.nextInt();
		
		switch (thang) {
		case 1,3,5,7,8,10,12: {
			System.out.println("Co 31 ngay!");
			break;
		}
		case 4,6,9,11:{
			System.out.println("Co 30 ngay!");
			break;
		}
		case 2:{
			if (nam%4==0  || nam%400==0) {
				System.out.println("Co 29 ngay!");
			}else {
				System.out.println("Co 28 ngay!");
			}
			break;
		}
		default:
			System.out.println("Nhap thang ngu!");
		}
		
	}
}