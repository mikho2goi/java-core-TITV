import java.util.Scanner;

public class ViDu {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("Bang cuu chuong tu 1 den 9!");
	
	for (int i = 1; i <= 9; i++) {
		for (int j = 1; j < 9; j++) {
			System.out.println(i+" x "+j+" = "+(i*j));
		}
		System.out.println("---------");
	}
}
}
